    "from typing import List\n",
    "class Solution:\n",
    "    def closedIsland(self, grid: List[List[int]]) -> int:\n",
    "        if len(grid) == 0 or len(grid[0]) == 0:\n",
    "            return 0\n",
    "        R = len(grid)\n",
    "        C = len(grid[0])\n",
    "        res = 0\n",
    "        visited = set()\n",
    "\n",
    "        def bfs(r: int, c: int) -> int:\n",
    "            if (r, c) in visited or grid[r][c] == 1:\n",
    "                return 0\n",
    "            q = collections.deque()\n",
    "            q.append((r,c))\n",
    "            curVi = set()\n",
    "            isQulify = True\n",
    "            while q:\n",
    "                x,y = q.popleft()\n",
    "                curVi.add((x,y))\n",
    "                for dx, dy in (0,1), (0,-1), (-1,0), (1,0):\n",
    "                    nx = x+dx\n",
    "                    ny = y+dy\n",
    "                    if nx < 0 or nx > R-1 or ny < 0 or ny > C-1:\n",
    "                        isQulify = False\n",
    "                    elif (nx, ny) in curVi:\n",
    "                        continue\n",
    "                    elif grid[nx][ny] == 0:\n",
    "                        q.append((nx,ny))\n",
    "            visited.update(curVi)\n",
    "            return 1 if isQulify else 0\n",
    "\n",
    "        for r in range(R):\n",
    "            for c in range(C):\n",
    "                res += bfs(r, c)\n",
    "\n",
    "        return res "
