    "import collections\n",
    "from typing import Tuple\n",
    "class Solution:\n",
    "    def countOfAtoms(self, formula: str) -> str:\n",
    "        def parse(idx: int) -> Tuple[int, dict]:\n",
    "            eleCnt = collections.defaultdict(int)\n",
    "            while idx < len(formula):\n",
    "                if formula[idx].isalpha():\n",
    "                    end = idx + 1\n",
    "                    while end < len(formula) and formula[end].isalpha() and formula[end].islower():\n",
    "                        end += 1\n",
    "                    ele = formula[idx:end]\n",
    "                    idx = end\n",
    "                    cnt = 1\n",
    "                    if idx < len(formula) and formula[idx].isdigit():\n",
    "                        end = idx + 1\n",
    "                        while end < len(formula) and  formula[end].isdigit():\n",
    "                            end += 1\n",
    "                        cnt = int(formula[idx:end])\n",
    "                        idx = end\n",
    "                    eleCnt[ele] += cnt\n",
    "                elif formula[idx] == '(':\n",
    "                    idx, subCnt = parse(idx+1)\n",
    "                    for e, c in subCnt.items():\n",
    "                        eleCnt[e] += c\n",
    "                elif formula[idx] == ')':\n",
    "                    multiple = 1\n",
    "                    end = idx + 1\n",
    "                    while end < len(formula) and formula[end].isdigit():\n",
    "                        end += 1\n",
    "                    if end > idx +1:\n",
    "                        multiple = int(formula[idx+1:end])\n",
    "                    for e in eleCnt:\n",
    "                        eleCnt[e] *= multiple\n",
    "                    return end, eleCnt\n",
    "            return idx, eleCnt\n",
    "\n",
    "        _,fullCnt = parse(0)\n",
    "        sortedEles = sorted(fullCnt.keys())\n",
    "        result = []\n",
    "        for e in sortedEles:\n",
    "            result.append(e)\n",
    "            if fullCnt[e] > 1:\n",
    "                result.append(str(fullCnt[e]))\n",
    "        return ''.join(result)\n",
    "                "
